{
  "configurations": [
    {
      "name": "run web api",
      "request": "launch",
      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "deno"
      },
      "program": "src/main.ts",
      "cwd": "${workspaceFolder}/web_api",
      "sourceMaps": true,
      "runtimeArgs": ["run", "--no-lock", "-c", "deno.json", "--env"],
      "runtimeExecutable": "deno"
    }
  ]
}
