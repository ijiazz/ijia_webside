{
  "type": "module",
  "scripts": {
    "init-deps": "pnpm run init-ijia-data & pnpm run init-api-types",
    "init-api-types": "pnpm run --filter @ijia/api-types build",
    "init-ijia-data": "tsc -p deps/ijia-data/tsconfig.json",
    "check-type": "pnpm run -r check-type",
    "check-fmt": "pnpm prettier --check web_api web e2e"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.27.0",
  "publishConfig": {
    "registry": "https://registry.npm.org"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "commander": "^13.1.0",
    "prettier": "^3.5.2",
    "rollup": "^4.30.8",
    "tslib": "^2.7.0",
    "typescript": "^5.6.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "esbuild"
    ]
  }
}
